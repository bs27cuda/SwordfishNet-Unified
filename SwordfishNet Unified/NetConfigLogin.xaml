<Window x:Class="SwordfishNet_Unified.NetConfigLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SwordfishNet"
        mc:Ignorable="d"
        Title="Network config" Height="100" Width="300"
        ResizeMode="NoResize"
        Background="{StaticResource Background}"
        SizeToContent="Height">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="36"/>
        </Grid.RowDefinitions>
        <PasswordBox x:Name="NetConfigPasswordBox" Grid.Row="0" VerticalAlignment="Bottom" Height="30" KeyDown="NetConfigPasswordBox_KeyDown"/>
        <Button x:Name="GoButton" Content="Submit" Grid.Row="2" Click="GoButton_Click"/>
    </Grid>
</Window>
